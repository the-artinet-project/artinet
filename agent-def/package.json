{
  "name": "agent-def",
  "version": "0.1.0",
  "description": "A standardized definition for collaborative agents.",
  "main": "dist/src/definition.js",
  "module": "dist/src/definition.js",
  "types": "dist/src/definition.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/definition.d.ts",
      "import": "./dist/src/definition.js",
      "default": "./dist/src/definition.js"
    }
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "generate:schema": "tsx ./scripts/generate-schema.ts",
    "lint": "eslint .",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm i && npm run generate:schema && npm run build"
  },
  "keywords": [
    "artinet",
    "agent",
    "definition",
    "configuration",
    "ai",
    "artificial intelligence"
  ],
  "author": "artinet",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-artinet-project/artinet.git"
  },
  "bugs": {
    "url": "https://github.com/the-artinet-project/artinet/issues"
  },
  "homepage": "https://artinet.io",
  "files": [
    "./dist/**/*",
    "./package.json",
    "./README.md"
  ],
  "engines": {
    "node": ">=18.9.1"
  },
  "rootDir": ".",
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/node": "^22.15.3",
    "eslint": "^9.25.1",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "zod-to-json-schema": "^3.25.0"
  },
  "dependencies": {
    "@artinet/sdk": "^0.6.0",
    "@artinet/types": "^0.0.9",
    "zod": "^3.25"
  }
}
