{
  "name": "@artinet/ask",
  "version": "0.1.0",
  "description": "A lightweight command-line chat client for connecting to A2A Servers.",
  "author": "artinet",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/the-artinet-project/artinet"
  },
  "issues": {
    "url": "https://github.com/the-artinet-project/artinetissues"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "ask": "./dist/src/cli.js"
  },
  "keywords": [
    "ask",
    "artinet",
    "chat",
    "a2a",
    "agent2agent",
    "agent",
    "ai",
    "artificial intelligence"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "type": "module",
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "rootDir": ".",
  "scripts": {
    "prepublishOnly": "npm run clean:all && npm i && npm run build",
    "build": "tsc --project tsconfig.json",
    "clean": "rimraf dist",
    "clean:all": "rimraf dist node_modules package-lock.json",
    "dev": "tsx src/cli.ts",
    "dev:server": "tsx src/test-server.ts",
    "ask": "node dist/src/cli.js"
  },
  "dependencies": {
    "@artinet/sdk": "^0.6.4",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "prompts": "^2.4.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@types/prompts": "^2.4.9",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
