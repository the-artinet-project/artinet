{
  "name": "@artinet/fleet",
  "version": "0.1.0-alpha.1",
  "description": "A an agentic orchestration server for on premise deployment.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./express": {
      "types": "./dist/server/express/index.d.ts",
      "import": "./dist/server/express/index.js",
      "default": "./dist/server/express/index.js"
    }
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint .",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "artinet",
    "a2a",
    "mcp",
    "agent",
    "deployment",
    "orchestration",
    "artificial intelligence"
  ],
  "author": "artinet",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-artinet-project/fleet.git"
  },
  "bugs": {
    "url": "https://github.com/the-artinet-project/fleet/issues"
  },
  "homepage": "https://github.com/the-artinet-project/fleet#readme",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "rootDir": ".",
  "dependencies": {
    "@artinet/armada": "^0.1.0-alpha.1",
    "@artinet/sdk": "^0.6.0-preview.1",
    "@artinet/types": "^0.0.9-alpha.2",
    "@sindresorhus/slugify": "^3.0.0",
    "agent-def": "^0.0.1",
    "orc8": "^0.0.20-alpha.1",
    "zod": "^3.25"
  },
  "peerDependencies": {
    "@modelcontextprotocol/sdk": "^1.24.3",
    "@opentelemetry/api": "^1.9.0",
    "express": "^5.2.1",
    "openai": "^6.15.0"
  },
  "devDependencies": {
    "@artinet/loader": "^0.0.1",
    "@eslint/js": "^9.25.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.3",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.25.1",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "pino": "^10.1.0",
    "rimraf": "^5.0.5",
    "supertest": "latest",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  },
  "engines": {
    "node": ">=18.9.1"
  }
}
